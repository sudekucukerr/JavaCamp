package homeworkExercises.week3;

public class CustomerManager {
	
	private Customer customer;
	
	public CustomerManager(Customer customer) {
		this.customer = customer;
	}
	
	public void Save() {
		System.out.println("Müşteri kaydedildi. " + customer.FirstName + " " + customer.LastName);
	}

	public void Delete() {
		System.out.println("Müşteri silindi. " + customer.FirstName + " " + customer.LastName);
	}
	
	
}
